{
  "mcpServers": {
    "Atlassian-Remote": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://mcp.atlassian.com/v1/sse"]
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp", "--transport", "stdio"]
    },
    "github": {
      "command": "pnpm",
      "args": ["exec", "github-mcp-server-mcp"]
    },
    "mongodb": {
      "command": "npx",
      "args": ["-y", "mongodb-mcp-server"]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/frx33355/Documents/dev/millenium"
      ]
    },
    "shell": {
      "command": "node",
      "args": ["./mcp/shell/index.js"]
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp@latest"]
    },
    "snowflake": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "snowflake-labs-mcp",
        "--service-config-file",
        "/Users/frx33355/.config/snowflake/tools_config.yaml",
        "--connection-name",
        "ml_prod"
      ],
      "env": {}
    }
  }
}
